{
  "name": "New Tab New Ideas",
  "version": "0.0.1",
  "description": "Replace new tab page with a minimal motivational quote",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 3,
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "browser_action": {
    "default_icon": "/icons/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "author": {
    "name": "Atul Joshy",
    "url": "https://atuljoshy.ml"
  }
  
}
